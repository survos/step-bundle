{
  "name": "survos/step-bundle",
  "description": "Author steps as typed actions (Bash/Console/OpenUrl/DisplayCode/etc.) and render to README/Reveal/JSON.",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony-ux"
  ],
  "require": {
    "php": "^8.4",
    "symfony/http-kernel": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/config": "^7.3",
    "survos/core-bundle": "^2.0",
    "symfony/asset-mapper": "^7.3",
    "survos/installer": "^2.1",
    "survos/jsonl-bundle": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Survos\\StepBundle\\": "src/"
    },
    "files": ["src/Runtime/functions.php"]
  },
  "config": {
    "allow-plugins": {
      "survos/installer": true
    }
  }
}
