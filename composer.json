{
    "name": "survos/step-bundle",
    "description": "Author steps as typed actions (Bash/Console/OpenUrl/DisplayCode/etc.) and render to README/Reveal/JSON.",
    "keywords": [
        "symfony-ux"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/http-kernel": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/config": "^7.3||^8.0",
        "survos/core-bundle": "^2.0",
        "symfony/asset-mapper": "^7.3||^8.0",
        "survos/installer": "^2.0",
        "survos/jsonl-bundle": "dev-main",
        "symfony/framework-bundle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Survos\\StepBundle\\": "src/"
        },
        "files": [
            "src/Runtime/functions.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true
        }
    },
    "require-dev": {
        "symfony/panther": "^2.2"
    }
}
